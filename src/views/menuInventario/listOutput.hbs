<div class="container-fluid">
    <div class="row mt-4">
        <main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
            <div class="col-6 mx-auto">{{>message}}</div>
            <div class="float-left ml-5"><h3>SALIDAS INVENTARIO</h3></div>
            <div class="table-responsive-lg">
                <table id="movementsTable" class="table table-sm table-striped table-bordered mt-4 bg-white">
                    <thead style="background-color: rgb(92, 135, 230); font-size: 1.15rem;">
                        <tr>
                            <th scope="col">Id</th>
                            <th scope="col">Area</th>
                            <th scope="col">Empleado</th>
                            <th scope="col">Elemento</th>
                            <th scope="col">Cantidad</th>
                            <th scope="col">Fecha</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each outputRows}}
                        <tr>
                            <th scope="row" id="idDevolucion">{{IdMovimiento}}</th>
                            <td>{{NombreArea}}</td>
                            <td>{{NombreEmpleado}} {{ApellidoEmpleado}}</td>
                            <td>{{NombreElemento}}</td>
                            <td>{{Cantidad}}</td>
                            <td>{{Fecha}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </main>
    </div>
</div>